#!/bin/bash
####################################################################################################
# 環境設定
####################################################################################################
# 環境の設定
unset BASE_DIR
# PATHの設定
PATH="${PATH_BACKUP}"
unset PATH_BACKUP

####################################################################################################
# ターミナルの設定
####################################################################################################

# --------------------------------------------------------------------------------------------------
# PS1の設定
# --------------------------------------------------------------------------------------------------
export PS1=${PS1_BACKUP}
unset PS1_BACKUP

# 行末オートワープ機能(折返し機能)の有効化
printf '\033[?7h'

####################################################################################################
# 後処理
####################################################################################################
unset IS_ACTIVATED
